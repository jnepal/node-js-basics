<div class="socialmediaicons">
  <span class="title">Join the movement: </span>
  <% appdata.social.forEach(function(item) { %>
    <ul class="group">
  	 <li><a href="<%= item.url %>" target="_blank"><img class="icons" src="<%= item.img%>" alt="icon for<%= item.shorname%>"/></a></li>
    </ul>
  <% }); %>
</div>
